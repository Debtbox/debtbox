{
  "name": "debtbox",
  "private": true,
  "version": "0.0.1-dev",
  "type": "module",
  "homepage": "https://debtbox.sa",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d dist",
    "security:test": "node scripts/security-test.js",
    "security:test:advanced": "node scripts/security-test-advanced.js",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:check": "pnpm run security:test:advanced && pnpm run security:audit",
    "security:fix": "pnpm audit --fix",
    "test:security": "pnpm run security:test:advanced"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.90.1",
    "@types/node": "^24.5.2",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "i18next-icu": "^2.4.0",
    "js-cookie": "^3.0.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.13",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  }
}
